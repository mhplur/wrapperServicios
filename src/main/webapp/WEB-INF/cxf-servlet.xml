<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:cxf="http://cxf.apache.org/core"
	xmlns:soap="http://cxf.apache.org/bindings/soap"
	xsi:schemaLocation="http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://cxf.apache.org/bindings/soap http://cxf.apache.org/schemas/configuration/soap.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<jaxws:server id="rcService"
		serviceClass="ec.gob.agricultura.dsii.sw.ServiciosInternos"
		address="/rcService">
		<jaxws:serviceBean>
			<bean class="ec.gob.agricultura.dsii.sw.ServiciosInternosImpl" />

		</jaxws:serviceBean>

	</jaxws:server>

	<jaxws:server id="sriService"
		serviceClass="ec.gob.agricultura.dsii.sw.SRIService"
		address="/sriService">
		<jaxws:serviceBean>
			<bean class="ec.gob.agricultura.dsii.sw.SRIServiceImpl" />

		</jaxws:serviceBean>

	</jaxws:server>


	<jaxws:server id="personasNaturalesService"
		serviceClass="ec.gob.agricultura.dsii.sw.ServicioPersonasNaturales"
		address="/personasNaturalesService">
		<jaxws:serviceBean>
			<bean
				class="ec.gob.agricultura.dsii.sw.ServicioPersonasNaturalesImp" />
		</jaxws:serviceBean>
	</jaxws:server>
